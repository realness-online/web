{
  "name": "Realness",
  "version": "0.5.40",
  "private": false,
  "description": "Source code for https://realness.online. this code is open source so that we can keep our commitment to transparency",
  "author": "Scott Fryxell <sfryxell@me.com>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "deploy": "firebase use production && yarn build && firebase deploy --only hosting,storage",
    "build:staging": "vue-cli-service build --mode staging",
    "deploy:staging": "firebase use staging && yarn build:staging && firebase deploy --only hosting,storage",
    "test:unit:watch": "vue-cli-service test:unit --watchAll"
  },
  "dependencies": {
    "@realness.online/web-workers": "^0.3.0",
    "firebase": "^7.8.2",
    "libphonenumber-js": "^1.7.44",
    "register-service-worker": "1.6.2",
    "vue": "2.6.11",
    "vue-router": "^3.1.5"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.2.2",
    "@vue/cli-plugin-e2e-cypress": "~4.2.2",
    "@vue/cli-plugin-eslint": "~4.2.2",
    "@vue/cli-plugin-pwa": "~4.2.2",
    "@vue/cli-plugin-unit-jest": "~4.2.2",
    "@vue/cli-service": "~4.2.2",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "^1.0.0-beta.31",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^25.1.0",
    "core-js": "^3.6.4",
    "eslint": "^6.4.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.1",
    "firebase-tools": "^7.13.1",
    "flush-promises": "^1.0.2",
    "jest-fetch-mock": "^3.0.1",
    "jest-localstorage-mock": "^2.4.0",
    "jest-serializer-vue": "^2.0.2",
    "jsdom-worker-fix": "^0.1.8",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.6.11",
    "vue-test-utils": "^1.0.0-beta.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "camelcase": 0,
      "space-before-function-paren": 0,
      "indent": "off"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 2%"
  ],
  "jest": {
    "verbose": false,
    "collectCoverage": false,
    "restoreMocks": false,
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "setupFiles": [
      "<rootDir>/tests/unit/setup.js",
      "<rootDir>/tests/polyfill/createRange.js",
      "<rootDir>/tests/polyfill/scrollIntoView.js",
      "<rootDir>/tests/polyfill/IntersectionObserver.js",
      "<rootDir>/tests/polyfill/FileReaderSync.js",
      "jest-localstorage-mock",
      "jsdom-worker-fix"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "coverageDirectory": "<rootDir>/tests/unit/coverage",
    "cacheDirectory": "<rootDir>/tests/unit/cache",
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!src/main.js",
      "!src/service.worker.js",
      "!src/register_service_worker.js",
      "!src/router.js",
      "!**/node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  },
  "license": "UNLICENSED"
}
