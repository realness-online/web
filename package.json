{
  "name": "@realness.online/web",
  "version": "1.3.4",
  "private": false,
  "type": "module",
  "description": "When people search for 'outsider social network' they'll find realness online",
  "author": "Scott Fryxell <sfryxell@me.com>",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "deploy": "yarn lint && yarn format && yarn test --coverage --verbose && yarn build && firebase deploy --only hosting,storage -m $npm_package_version",
    "format": "prettier .  --write",
    "format:check": "prettier . --check",
    "release": "gh release create $npm_package_version && yarn deploy",
    "release:npm": "yarn publish",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "workers": "webpack --config workers.config.js",
    "workers:dev": "yarn workers --mode=development --watch"
  },
  "dependencies": {
    "@davidmarkclements/hsl-to-hex": "1.0.2",
    "@realness.online/potrace": "../potrace/",
    "@vueuse/core": "8.1.2",
    "firebase": "9.6.9",
    "hsl-to-hex": "1.0.0",
    "idb-keyval": "6.1.0",
    "jimp": "0.16.1",
    "libphonenumber-js": "1.9.50",
    "node-vibrant": "3.2.1-alpha.1",
    "object-hash": "3.0.0",
    "rgb-hex": "4.0.0",
    "svgo": "1.3.0",
    "vue": "3.2.31",
    "vue-router": "4.0.14"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "2.2.4",
    "@vue/test-utils": "2.0.0-rc.18",
    "c8": "7.11.0",
    "eslint": "8",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-standard": "5.0.0",
    "eslint-plugin-vue": "8",
    "fake-indexeddb": "4.0.0-beta.2",
    "firebase-tools": "10.4.2",
    "happy-dom": "2.50.0",
    "mockdate": "3.0.5",
    "prettier": "2.6.0",
    "process": "0.11.10",
    "simplex-noise": "3.0.1",
    "stylus": "0.57.0",
    "vite": "2.8.6",
    "vite-plugin-package-version": "1.0.2",
    "vitest": "0.7.10",
    "vue-router-mock": "0.1.7"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "es2021": true,
      "jest": true,
      "serviceworker": true,
      "worker": true
    },
    "globals": {
      "defineProps": "readonly",
      "defineEmits": "readonly",
      "defineExpose": "readonly",
      "withDefaults": "readonly"
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "no-import-assign": "off",
      "camelcase": 0,
      "indent": "off",
      "lines-between-class-members": 0,
      "space-before-function-paren": [
        "error",
        {
          "anonymous": "never",
          "named": "never",
          "asyncArrow": "always"
        }
      ],
      "vue/multi-word-component-names": "off",
      "vue/require-default-prop": "off",
      "vue/prop-name-casing": [
        "error",
        "snake_case"
      ],
      "vue/singleline-html-element-content-newline": "off",
      "vue/html-closing-bracket-spacing": [
        "error",
        {
          "selfClosingTag": "always"
        }
      ],
      "vue/html-closing-bracket-newline": [
        "error",
        {
          "singleline": "never",
          "multiline": "never"
        }
      ]
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSameLine": true,
    "arrowParens": "avoid",
    "vueIndentScriptAndStyle": true
  },
  "browserslist": [
    "last 2 versions"   
  ],
  "license": "UNLICENSED"
}
