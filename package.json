{
  "name": "@realness.online/web",
  "version": "1.5.12",
  "private": false,
  "type": "module",
  "keywords": [
    "Private",
    "Realtime Graphics",
    "animation",
    "Social Network",
    "svg",
    "computer-graphics",
    "vector-graphics",
    "web-programming"
  ],
  "description": "When people search for 'outsider social network' they'll find realness online. The medium is the message",
  "author": "Scott Fryxell <sfryxell@gmail.com>",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "serve:https": "serve dist",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix",
    "deploy:realness": "pnpm lint && pnpm format && pnpm workers && pnpm build && firebase deploy --only hosting,storage -m $npm_package_version",
    "format": "prettier .  --write",
    "format:check": "prettier . --check",
    "release": "pnpm release:gh && pnpm release:npm && git tag v$npm_package_version",
    "release:npm": "pnpm publish",
    "release:gh": "gh release create $npm_package_version",
    "workers": "webpack --config workers.config.cjs",
    "workers:dev": "pnpm workers --mode=development --watch",
    "postinstall": "husky install",
    "prepack": "pinst --disable"
  },
  "dependencies": {
    "@davidmarkclements/hsl-to-hex": "1.0.2",
    "@headlessui/vue": "^1.7.19",
    "@heroicons/vue": "^2.1.3",
    "@realness.online/potrace": "link:../potrace",
    "@realness.online/vtracer": "link:../vtracer",
    "@vueuse/core": "10.9.0",
    "culori": "^4.0.1",
    "events": "^3.3.0",
    "exifreader": "^4.21.1",
    "firebase": "10.9.0",
    "hsl-to-hex": "1.0.0",
    "idb-keyval": "6.2.1",
    "jimp": "0.16.2",
    "libphonenumber-js": "1.10.58",
    "node-vibrant": "3.2.1-alpha.1",
    "object-hash": "3.0.0",
    "pako": "^2.1.0",
    "rgb-hex": "4.1.0",
    "svgo": "^3.2.0",
    "url": "^0.11.3",
    "vue": "3.4.21",
    "vue-router": "4.3.0",
    "workbox-build": "^7.0.0",
    "workbox-window": "^7.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "assert": "2.1.0",
    "browserify-fs": "1.0.0",
    "browserify-zlib": "0.2.0",
    "buffer": "^6.0.3",
    "c8": "9.1.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-vue": "^9.23.0",
    "fake-indexeddb": "5.0.2",
    "happy-dom": "14.3.6",
    "https-browserify": "1.0.0",
    "https-localhost": "^4.7.1",
    "husky": "^9.0.11",
    "mockdate": "3.0.5",
    "os-browserify": "0.3.0",
    "path-browserify": "1.0.1",
    "pinst": "^3.0.0",
    "prettier": "3.2.5",
    "process": "0.11.10",
    "querystring-es3": "^0.2.1",
    "simplex-noise": "4.0.1",
    "stream-browserify": "3.0.0",
    "stream-http": "3.2.0",
    "stylus": "0.63.0",
    "util": "0.12.5",
    "vite": "5.2.6",
    "vite-plugin-package-version": "1.1.0",
    "vite-plugin-pwa": "^0.19.7",
    "vitest": "1.4.0",
    "webpack": "5.91.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "5.1.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "es2021": true,
      "jest": true,
      "serviceworker": true,
      "worker": true
    },
    "globals": {
      "process": "readonly",
      "defineProps": "readonly",
      "defineEmits": "readonly",
      "defineExpose": "readonly",
      "withDefaults": "readonly"
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-recommended",
      "prettier"
    ],
    "rules": {
      "no-import-assign": "off",
      "camelcase": 0,
      "indent": "off",
      "lines-between-class-members": 0,
      "vue/multi-word-component-names": "off",
      "vue/require-default-prop": "off",
      "vue/prop-name-casing": [
        "error",
        "snake_case"
      ],
      "vue/singleline-html-element-content-newline": "off",
      "vue/html-closing-bracket-spacing": [
        "error",
        {
          "selfClosingTag": "always"
        }
      ],
      "vue/html-closing-bracket-newline": [
        "error",
        {
          "singleline": "never",
          "multiline": "never"
        }
      ]
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSameLine": true,
    "arrowParens": "avoid",
    "vueIndentScriptAndStyle": true
  },
  "browserslist": [
    "last 2 versions"
  ],
  "license": "UNLICENSED"
}
