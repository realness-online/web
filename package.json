{
  "name": "@realness.online/web",
  "version": "1.7.0",
  "private": false,
  "type": "module",
  "keywords": [
    "svg",
    "computer-graphics",
    "vector-graphics",
    "Private",
    "Realtime Graphics",
    "animation",
    "Social Network",
    "potrace",
    "trace",
    "tracing",
    "posterization"
  ],
  "description": "When people search for 'outsider social network' they'll find realness online. The medium is the message",
  "author": "Scott Fryxell <sfryxell@gmail.com>",
  "scripts": {
    "dev": "vite",
    "dev:workers": "rollup -c workers.config.js --watch",
    "build": "vite build",
    "build:workers": "rollup -c workers.config.js",
    "serve": "vite preview",
    "serve:https": "serve dist",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix",
    "deploy": "npm run lint && npm run format && npm run build:workers && npm run build && firebase deploy --only hosting,storage -m $npm_package_version",
    "format": "prettier .  --write",
    "format:check": "prettier . --check",
    "release": "npm run release:gh && npm run release:npm && git tag v$npm_package_version",
    "release:npm": "npm publish",
    "release:gh": "gh release create $npm_package_version",
    "prepare": "husky",
    "docs": "jsdoc -c jsdoc.json"
  },
  "dependencies": {
    "@davidmarkclements/hsl-to-hex": "1.0.2",
    "@vueuse/core": "12.2.0",
    "culori": "4.0.1",
    "exifreader": "4.26.0",
    "firebase": "11.1.0",
    "hsl-to-hex": "1.0.0",
    "idb-keyval": "6.2.1",
    "libphonenumber-js": "1.11.17",
    "node-vibrant": "4.0.0",
    "object-hash": "3.0.0",
    "rgb-hex": "4.1.0",
    "vue": "3.5.13",
    "vue-router": "4.5.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "5.1.1",
    "@rollup/plugin-commonjs": "28.0.2",
    "@rollup/plugin-node-resolve": "16.0.0",
    "@vitejs/plugin-vue": "5.2.1",
    "eslint": "9.17.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-vue": "9.32.0",
    "fake-indexeddb": "6.0.0",
    "happy-dom": "16.2.6",
    "husky": "9.1.7",
    "jsdoc": "^4.0.4",
    "mockdate": "3.0.5",
    "pako": "2.1.0",
    "prettier": "3.4.2",
    "prettier-eslint": "16.3.0",
    "rollup": "^4.29.1",
    "rollup-plugin-analyzer": "4.0.0",
    "stylus": "0.64.0",
    "svgo": "3.3.2",
    "vite": "6.0.6",
    "vite-plugin-package-version": "1.1.0",
    "vite-plugin-pwa": "0.21.1",
    "vitest": "2.1.8",
    "workbox-build": "7.3.0",
    "workbox-window": "7.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "es2021": true,
      "jest": true,
      "serviceworker": true,
      "worker": true
    },
    "globals": {
      "process": "readonly",
      "defineProps": "readonly",
      "defineEmits": "readonly",
      "defineExpose": "readonly",
      "withDefaults": "readonly"
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-recommended",
      "prettier"
    ],
    "rules": {
      "no-import-assign": "off",
      "camelcase": 0,
      "indent": "off",
      "lines-between-class-members": 0,
      "vue/multi-word-component-names": "off",
      "vue/require-default-prop": "off",
      "vue/prop-name-casing": [
        "error",
        "snake_case"
      ],
      "vue/singleline-html-element-content-newline": "off",
      "vue/html-closing-bracket-spacing": [
        "error",
        {
          "selfClosingTag": "always"
        }
      ],
      "vue/html-closing-bracket-newline": [
        "error",
        {
          "singleline": "never",
          "multiline": "never"
        }
      ]
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSameLine": true,
    "arrowParens": "avoid",
    "vueIndentScriptAndStyle": true
  },
  "browserslist": [
    "last 2 versions"
  ],
  "license": "UNLICENSED"
}
