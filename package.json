{
  "name": "@realness.online/web",
  "version": "1.4.98",
  "private": false,
  "type": "module",
  "keywords": [
    "svg",
    "computer-graphics",
    "vector-graphics",
    "web-programming"
  ],
  "description": "When people search for 'outsider social network' they'll find realness online",
  "author": "Scott Fryxell <sfryxell@gmail.com>",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "serve:https": "serve dist",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix",
    "deploy": "yarn lint && yarn format && yarn workers && yarn build && firebase deploy --only hosting,storage -m $npm_package_version",
    "format": "prettier .  --write",
    "format:check": "prettier . --check",
    "release": "yarn release:gh && yarn release:npm && git tag v$npm_package_version",
    "release:npm": "yarn publish",
    "release:gh": "gh release create $npm_package_version",
    "workers": "webpack --config workers.config.cjs",
    "workers:dev": "yarn workers --mode=development --watch",
    "postinstall": "husky install",
    "prepack": "pinst --disable"
  },
  "dependencies": {
    "@davidmarkclements/hsl-to-hex": "1.0.2",
    "@headlessui/vue": "^1.5.0",
    "@heroicons/vue": "^1.0.6",
    "@realness.online/potrace": "../potrace/",
    "@realness.online/vtracer": "../vtracer/",
    "@vueuse/core": "9.8.1",
    "culori": "^3.2.0",
    "events": "^3.3.0",
    "exifreader": "^4.13.0",
    "firebase": "10.7.2",
    "hsl-to-hex": "1.0.0",
    "idb-keyval": "6.1.0",
    "jimp": "0.16.2",
    "libphonenumber-js": "1.9.53",
    "node-vibrant": "3.2.1-alpha.1",
    "object-hash": "3.0.0",
    "pako": "^2.0.4",
    "querystring-es3": "^0.2.1",
    "rgb-hex": "4.0.0",
    "svgo": "^2.8.0",
    "url": "^0.11.3",
    "vue": "3.2.36",
    "vue-router": "4.0.15",
    "webpack-bundle-analyzer": "^4.10.1",
    "workbox-build": "^6.5.4",
    "workbox-window": "^6.5.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.4.0",
    "assert": "2.0.0",
    "browserify-fs": "1.0.0",
    "browserify-zlib": "0.2.0",
    "buffer-browserify": "0.2.5",
    "c8": "7.11.2",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-vue": "8",
    "fake-indexeddb": "4.0.0-beta.2",
    "happy-dom": "3.2.0",
    "https-browserify": "1.0.0",
    "https-localhost": "^4.7.1",
    "husky": "^8.0.1",
    "mockdate": "3.0.5",
    "os-browserify": "0.3.0",
    "path-browserify": "1.0.1",
    "pinst": "^3.0.0",
    "prettier": "2.6.2",
    "process": "0.11.10",
    "simplex-noise": "3.0.1",
    "stream-browserify": "3.0.0",
    "stream-http": "3.2.0",
    "stylus": "0.57.0",
    "util": "0.12.4",
    "vite": "4.4.12",
    "vite-plugin-package-version": "1.0.2",
    "vite-plugin-pwa": "^0.16.5",
    "vitest": "^0.34.5",
    "webpack": "5.72.1",
    "webpack-cli": "4.9.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "es2021": true,
      "jest": true,
      "serviceworker": true,
      "worker": true
    },
    "globals": {
      "defineProps": "readonly",
      "defineEmits": "readonly",
      "defineExpose": "readonly",
      "withDefaults": "readonly"
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-recommended",
      "prettier"
    ],
    "rules": {
      "no-import-assign": "off",
      "camelcase": 0,
      "indent": "off",
      "lines-between-class-members": 0,
      "vue/multi-word-component-names": "off",
      "vue/require-default-prop": "off",
      "vue/prop-name-casing": [
        "error",
        "snake_case"
      ],
      "vue/singleline-html-element-content-newline": "off",
      "vue/html-closing-bracket-spacing": [
        "error",
        {
          "selfClosingTag": "always"
        }
      ],
      "vue/html-closing-bracket-newline": [
        "error",
        {
          "singleline": "never",
          "multiline": "never"
        }
      ]
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSameLine": true,
    "arrowParens": "avoid",
    "vueIndentScriptAndStyle": true
  },
  "browserslist": [
    "last 2 versions"
  ],
  "license": "UNLICENSED"
}
