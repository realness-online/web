{
  "name": "Realness",
  "description": "Source code for https://realness.online. this code is open source so that we can keep our commitment to transparency",
  "version": "0.4.12",
  "private": false,
  "author": "Scott Fryxell <sfryxell@me.com>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:staging": "vue-cli-service build --mode staging",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:watch": "vue-cli-service test:unit --watch",
    "deploy": "firebase use production && yarn build && firebase deploy",
    "deploy:staging": "firebase use staging && yarn build:staging && firebase deploy"
  },
  "dependencies": {
    "exif-js": "2.3.0",
    "firebase": "6.4.2",
    "javascript-potrace": "file:../javascript-potrace",
    "jimp": "0.6.8",
    "libphonenumber-js": "1.7.23",
    "register-service-worker": "1.6.2",
    "vue": "2.6.10",
    "vue-router": "3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-e2e-cypress": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-plugin-pwa": "^3.8.0",
    "@vue/cli-plugin-unit-jest": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "eslint": "^6.4.0",
    "eslint-plugin-vue": "^5.0.0",
    "firebase-tools": "^7.3.2",
    "flush-promises": "^1.0.2",
    "jest-fetch-mock": "^2.1.0",
    "jest-localstorage-mock": "^2.4.0",
    "jest-serializer-vue": "^2.0.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.5.21",
    "vue-test-utils": "^1.0.0-beta.11",
    "webpack-bundle-analyzer": "^3.5.0",
    "worker-loader": "^2.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "camelcase": 0,
      "space-before-function-paren": 0,
      "indent": "off"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 2%"
  ],
  "jest": {
    "verbose": false,
    "collectCoverage": false,
    "restoreMocks": false,
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "setupFiles": [
      "<rootDir>/tests/unit/setup.js",
      "<rootDir>/tests/polyfill/createRange.js",
      "<rootDir>/tests/polyfill/scrollIntoView.js",
      "<rootDir>/tests/polyfill/IntersectionObserver.js",
      "<rootDir>/tests/polyfill/FileReaderSync.js",
      "jest-localstorage-mock"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "coverageDirectory": "<rootDir>/tests/unit/coverage",
    "cacheDirectory": "<rootDir>/tests/unit/cache",
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!src/main.js",
      "!src/controller.js",
      "!src/register_service_worker.js",
      "!src/router.js",
      "!**/node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  }
}
