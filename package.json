{
  "name": "@realness.online/web",
  "version": "1.3.4",
  "private": false,
  "description": "When people search for 'outsider social network' they'll find realness online",
  "author": "Scott Fryxell <sfryxell@me.com>",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test:e2e": "cypress open",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "deploy": "yarn lint && yarn format && yarn test --coverage --verbose && yarn build && firebase deploy --only hosting,storage -m $npm_package_version",
    "format": "prettier .  --write",
    "format:check": "prettier . --check",
    "release": "gh release create $npm_package_version && yarn deploy",
    "release:npm": "yarn publish",
    "test": "TZ=UTC jest",
    "workers": "webpack --config workers.config.js",
    "workers:dev": "yarn workers --mode=development --watch"
  },
  "dependencies": {
    "@davidmarkclements/hsl-to-hex": "^1.0.2",
    "@realness.online/potrace": "../potrace/",
    "@vueuse/core": "^7.6.2",
    "core-js": "^3.21.1",
    "firebase": "^8.2.1",
    "hsl-to-hex": "^1.0.0",
    "idb-keyval": "^6.1.0",
    "jimp": "^0.16.1",
    "libphonenumber-js": "^1.7.44",
    "node-vibrant": "^3.2.1-alpha.1",
    "object-hash": "^3.0.0",
    "register-service-worker": "^1.7.1",
    "rgb-hex": "^4.0.0",
    "svgo": "^1.3.0",
    "vue": "^3.2.26",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.2.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-standard": "^6.0.0",
    "@vue/test-utils": "^2.0.0-rc.15",
    "eslint": "8",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "8",
    "fake-indexeddb": "^3.0.0",
    "firebase-tools": "^9.1.0",
    "jest": "^27.1.0",
    "jest-fetch-mock": "^3.0.1",
    "jest-localstorage-mock": "^2.4.0",
    "jest-serializer-vue": "^2.0.2",
    "jsdom-worker-fix": "git://github.com/scott-fryxell/jsdom-worker.git",
    "mockdate": "^3.0.2",
    "prettier": "^2.4.1",
    "process": "^0.11.10",
    "simplex-noise": "^3.0.0",
    "style-resources-loader": "^1.4.1",
    "stylus": "^0.56.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "vite": "^2.5.4",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-router-mock": "^0.1.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "es2021": true,
      "jest": true,
      "serviceworker": true,
      "worker": true
    },
    "globals": {
      "defineProps": "readonly",
      "defineEmits": "readonly",
      "defineExpose": "readonly",
      "withDefaults": "readonly"
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "no-import-assign": "off",
      "camelcase": 0,
      "indent": "off",
      "lines-between-class-members": 0,
      "space-before-function-paren": [
        "error",
        {
          "anonymous": "never",
          "named": "never",
          "asyncArrow": "always"
        }
      ],
      "vue/multi-word-component-names": "off",
      "vue/require-default-prop": "off",
      "vue/prop-name-casing": [
        "error",
        "snake_case"
      ],
      "vue/singleline-html-element-content-newline": "off",
      "vue/html-closing-bracket-spacing": [
        "error",
        {
          "selfClosingTag": "always"
        }
      ],
      "vue/html-closing-bracket-newline": [
        "error",
        {
          "singleline": "never",
          "multiline": "never"
        }
      ]
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSameLine": true,
    "arrowParens": "avoid",
    "vueIndentScriptAndStyle": true
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 2%"
  ],
  "jest": {
    "notify": false,
    "notifyMode": "change",
    "resetMocks": false,
    "automock": false,
    "bail": 0,
    "verbose": false,
    "collectCoverage": false,
    "restoreMocks": true,
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "setupFiles": [
      "<rootDir>/tests/unit/setup.js",
      "<rootDir>/tests/polyfill/activeElement.js",
      "<rootDir>/tests/polyfill/createrange.js",
      "<rootDir>/tests/polyfill/scrollIntoView.js",
      "<rootDir>/tests/polyfill/IntersectionObserver.js",
      "<rootDir>/tests/polyfill/FileReaderSync.js",
      "jest-localstorage-mock",
      "jsdom-worker-fix"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/unit/router.js"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "coverageDirectory": "<rootDir>/artifacts/unit/coverage",
    "cacheDirectory": "<rootDir>/artifacts/unit/cache",
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!src/main.js",
      "!src/workers/service.js",
      "!src/workers/register_service_worker.js",
      "!src/router.js",
      "!**/node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 90,
        "branches": 79.75,
        "functions": 89,
        "lines": 93
      }
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/"
  },
  "license": "UNLICENSED"
}
