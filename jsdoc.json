{
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.(js|vue)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "destination": "./docs/generated",
    "recurse": true,
    "readme": "README.md"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  }
}
