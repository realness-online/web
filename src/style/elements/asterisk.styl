*,
*::before
*::after
  box-sizing: border-box
  margin:0
  padding:0
  line-height: 1.33
  user-select: none

html *
  transition-timing-function: ease-out
  transition-duration: 0.25s
  transition-property: border, margin, padding, width, height, opacity, fill, stroke
  animation-duration: 0.5s
  animation-fill-mode: forwards
