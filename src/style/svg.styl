@require 'variables'
svg
  height: (base-line * 2)
  width: (base-line * 2)
  overflow: auto
  outline:none
  display: block
  fill: black
  stroke-width: 0
svg.working
  width: base-line * 3.3
  height: base-line
  animation-name: working
  animation-duration: 1.33s
  animation-delay: 0
  animation-iteration-count: infinite
